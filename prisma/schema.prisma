// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Assuntos {
  id        Int     @id @default(autoincrement())
  assunto   String
}

model Concursos {
  id        Int     @id @default(autoincrement())
  concurso  String  
}

model Perguntas {
  id          Int         @id @default(autoincrement())
  pergunta    String  
  a           String
  b           String
  c           String
  d           String
  e           String?
  resposta    String
  assunto     Assuntos    @relation(fields: [assuntoid], references: [id])
  assuntoid   Int       
  concurso    Concursos   @relation(fields: [concursoid], references: [id])
  concursoid  Int
}
